{
  "name": "eden-parfum-backend",
  "version": "1.0.0",
  "description": "Backend API for Eden Parfum perfume catalog",
  "main": "server.js",
  "scripts": {
    "start": "node server.js",
    "dev": "nodemon server.js",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "cache-bust": "node cache-busting-build.js build",
    "cache-bust:watch": "node cache-busting-build.js watch",
    "cache-bust:manifest": "node cache-busting-build.js manifest",
    "build": "npm run cache-bust",
    "build:watch": "npm run cache-bust:watch"
  },
  "keywords": [
    "perfume",
    "api",
    "nodejs",
    "express"
  ],
  "author": "Eden Parfum",
  "license": "MIT",
  "dependencies": {
    "cors": "^2.8.5",
    "dotenv": "^16.6.1",
    "express": "^4.18.2",
    "express-rate-limit": "^6.10.0",
    "firebase-admin": "^12.0.0",
    "helmet": "^7.0.0",
    "morgan": "^1.10.0",
    "nodemailer": "^7.0.6",
    "serverless-http": "^4.0.0",
    "winston": "^3.17.0",
    "winston-daily-rotate-file": "^5.0.0"
  },
  "devDependencies": {
    "jest": "^30.1.3",
    "jest-environment-jsdom": "^30.1.2",
    "nodemon": "^3.0.1",
    "supertest": "^7.1.4"
  }
}
